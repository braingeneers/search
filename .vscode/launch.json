{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Index",
            "cargo": {
                "args": [
                    "build",
                    "--bin=search",
                    "--package=search"
                ],
                "filter": {
                    "name": "search",
                    "kind": "bin"
                }
            },
            "args": [
                "--nocapture",
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "debug"
            }
        }
    ]
}